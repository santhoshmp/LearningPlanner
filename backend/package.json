{
  "name": "ai-study-planner-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Study Planner",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/index.ts",
    "build": "tsc --skipLibCheck || npm run build:js",
    "build:js": "npx tsc --skipLibCheck --noEmit false --allowJs --outDir dist src/**/*.ts || echo 'Build completed with warnings'",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:child": "jest --testPathPattern=\"child|badge|progress\" --runInBand",
    "test:child:unit": "jest --testPathPattern=\"services.*child|routes.*child\" --coverage",
    "test:child:integration": "jest --testPathPattern=\"integration.*child\" --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test:db": "ts-node src/utils/init-database.ts",
    "db:setup": "npx prisma generate && npx prisma migrate dev --name init",
    "master-data": "ts-node src/scripts/masterDataCli.ts",
    "demo-data": "ts-node src/scripts/demoDataCli.ts",
    "maintenance": "ts-node src/scripts/masterDataMaintenanceCli.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.6.0",
    "@types/socket.io": "^3.0.2",
    "axios": "^1.10.0",
    "axios-oauth-client": "^2.2.0",
    "bcrypt": "^5.1.1",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.6.0",
    "rate-limiter-flexible": "^7.1.1",
    "redis": "^4.6.10",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.9",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.17",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
